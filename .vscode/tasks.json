{
	"version": "2.0.0",
	"tasks": [
		{
				"label": "build",
				"type": "shell",
				"command": "g++",
				"args": [
						"-std=c++17",
						"-g",
						"-O2",
						"${fileBasename}",
						"-o",
						"${fileBasenameNoExtension}.exe"
				],
				"group": {
						"kind": "build",
						"isDefault": true
				}
		}
	]
}